@model CalculateResult

<h1>Calulator</h1>
<h4>Here is your result</h4>
<hr />

<!-- Strongly typed -->
<p>
    The result is... @Model.Result
</p>

<!-- Loosely typed -->
<p>
    Calculation was started at <i>@ViewData["StartTime"]</i>
</p>

<p>
    and completed at <i>@ViewBag.EndTime</i>
</p>